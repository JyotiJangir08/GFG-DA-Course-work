print(a)
print(a-5)
a=a*2
print(a)
print(50*'+')
age=123
if age>=0 and age<18:
    print('underage')
elif age >100:
    print('person does not exist')
print(age>=18 and age<=100)
if age >= 18 and age<=100:
    print('eligible')
else:
    print('not eligible')
