# int = number
print(12)
# float = decimal numbers 
print(5.55)
# string = characters , alphanumeric , everything written in  double quotos
print ("jyoti" , True, False, "jangir", 10 , 0.5)
# boolean = true and false 
print(True, False,)
print()
print()
# print fn with multiple parameters
print(True, False, "jangir", 10 , 0.5)
print(type(34))
print()
print()
# 1. Arithmetic operator

print(10+ 5)
print(10-5)
print(10*5)
print(10/5)
print(10%5)
print(10//5)
print(10**5)
print()
print(10*'-')
print(10*'j')
print(10+True)
print(10**True)
print('jyoti'+' '+ 'jangir')
print(True+True)

# 2. comparison operator
print(10>5)
print(10>=5)
print(10<5)
print(10<=5)
print(10==5)
print(10!=5)
print(10*'--')
print('a'> 'A')
# this is beacause of the ASCII code of every number 
# logical operators 
# and 
print(10>5 and 10<15)
# or
# not 

# 3.typecasting ( converting something )
